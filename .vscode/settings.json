{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/*.pbo": true,
        "**/*.exe": true,
        "**/keys": true,
        "**/release": true,
        "**/__pycache__": true,
    },
    "files.associations": {
        "**/*.hpp": "ext",
        // "**/*.hpp": "cpp",
        "**/*.cpp": "ext",
        "**/XEH_PREP.hpp": "sqf",
        "*.rpt": "log",
        "init*.hpp": "sqf",
    },
    "search.exclude": {
        "**/venv": true,
    },
    "explorer.autoRevealExclude": {
        "**/venv": true
    },
    "sqf.enableACE3": false,
    "sqf.enableCBA": true,
}
